{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongo",
    "public": false
  },
  "AccessToken": {
    "dataSource": "mongo",
    "public": true
  },
  "UserCredential": {
    "dataSource": "mongo",
    "public": false
  },
  "UserIdentity": {
    "dataSource": "mongo",
    "public": false
  },
  "ACL": {
    "dataSource": "mongo",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongo",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongo",
    "public": false
  },
  "user": {
    "dataSource": "mongo",
    "public": true,
    "options": {
      "emailVerificationRequired": true
    }
  },
  "container": {
    "dataSource": "storage",
    "public": true
  },
  "salesSlip": {
    "dataSource": "mongo",
    "public": true
  }
}
